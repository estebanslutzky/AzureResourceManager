{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "type": "string",
      "metadata": {
        "description": "Username for the Virtual Machine."
      }
    },
    "adminPassword": {
      "defaultValue": null,
      "type": "securestring",
      "metadata": {
        "description": "Password for the Virtual Machine."
      }
    },
    "vmName": {
      "type": "string",
      "metadata": {
        "description": "Name Of VM"
      }
    },
    "VM_Size": {
      "type": "string",
      "defaultValue": "Standard_D2",
      "allowedValues": [
        "Standard_D1",
        "Standard_D2",
        "Standard_D3",
        "Standard_D4"
      ],
      "metadata": {
        "description": "Size of VM"
      }
    },
    "networkInterfaces_estebanvm494_name": {
      "defaultValue": "estebanvm494",
      "type": "String"
    },
    "networkSecurityGroups_EstebanVM_nsg_name": {
      "defaultValue": "EstebanVM-nsg",
      "type": "String"
    },
    "publicIPAddresses_EstebanVM_ip_name": {
      "defaultValue": "EstebanVM-ip",
      "type": "String"
    },
    "virtualNetworks_ProdataARM_vnet_name": {
      "defaultValue": "ProdataARM-vnet",
      "type": "String"
    },
    "storageAccounts_prodataarmdiag927_name": {
      "defaultValue": "prodataarmdiag927",
      "type": "String"
    },
    "storageAccounts_prodataarmdisks138_name": {
      "defaultValue": "prodataarmdisks138",
      "type": "String"
    },
    "IP_Adress": {
      "defaultValue": "10.0.0.4",
      "type": "String"
    },
   "DiskCache": {
    "type": "string",
    "defaultValue": "N",
    "allowedValues": [
      "Y",
      "N"
    ]
  },
  "sizeOf_E_Drive": {
    "type": "string",
    "defaultValue": "127",
    "metadata": {
      "description": "Size of E:/ drive in GB"
    }
  }
},
  "variables": {},
  "resources": [
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/ProdataARM/providers/Microsoft.Compute/virtualMachines/EstebanVM'.",
      "type": "Microsoft.Compute/virtualMachines",
      "name": "[parameters('virtualMachines_EstebanVM_name')]",
      "apiVersion": "2015-06-15",
      "location": "northeurope",
      "properties": {
        "hardwareProfile": {
          "vmSize": "Standard_D1_v2"
        },
        "storageProfile": {
          "imageReference": {
            "publisher": "MicrosoftWindowsServer",
            "offer": "WindowsServer",
            "sku": "2012-R2-Datacenter",
            "version": "latest"
          },
          "osDisk": {
            "name": "[parameters('virtualMachines_EstebanVM_name')]",
            "createOption": "FromImage",
            "vhd": {
              "uri": "[concat('https', '://', parameters('storageAccounts_prodataarmdisks138_name'), '.blob.core.windows.net', concat('/vhds/', parameters('virtualMachines_EstebanVM_name'),'20170110111105.vhd'))]"
            },
            "caching": "ReadWrite"
          },
          "dataDisks": []
        },
        "osProfile": {
          "computerName": "[parameters('virtualMachines_EstebanVM_name')]",
          "adminUsername": "esteban",
          "windowsConfiguration": {
            "provisionVMAgent": true,
            "enableAutomaticUpdates": true
          },
          "secrets": [],
          "adminPassword": "[parameters('virtualMachines_EstebanVM_adminPassword')]"
        },
        "networkProfile": {
          "networkInterfaces": [
            {
              "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_estebanvm494_name'))]"
            }
          ]
        }
      },
      "resources": [],
      "dependsOn": [
        "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_prodataarmdisks138_name'))]",
        "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_estebanvm494_name'))]"
      ]
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/ProdataARM/providers/Microsoft.Network/networkInterfaces/estebanvm494'.",
      "type": "Microsoft.Network/networkInterfaces",
      "name": "[parameters('networkInterfaces_estebanvm494_name')]",
      "apiVersion": "2016-03-30",
      "location": "northeurope",
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAddress": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('IP_Adress'))]",
              "privateIPAllocationMethod": "Dynamic",
              "publicIPAddress": {
                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_EstebanVM_ip_name'))]"
              },
              "subnet": {
                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_ProdataARM_vnet_name')), '/subnets/default')]"
              }
            }
          }
        ],
        "dnsSettings": {
          "dnsServers": []
        },
        "enableIPForwarding": false,
        "networkSecurityGroup": {
          "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_EstebanVM_nsg_name'))]"
        }
      },
      "resources": [],
      "dependsOn": [
        "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_EstebanVM_ip_name'))]",
        "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_ProdataARM_vnet_name'))]",
        "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_EstebanVM_nsg_name'))]"
      ]
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/ProdataARM/providers/Microsoft.Network/networkSecurityGroups/EstebanVM-nsg'.",
      "type": "Microsoft.Network/networkSecurityGroups",
      "name": "[parameters('networkSecurityGroups_EstebanVM_nsg_name')]",
      "apiVersion": "2016-03-30",
      "location": "northeurope",
      "properties": {
        "securityRules": [
          {
            "name": "default-allow-rdp",
            "properties": {
              "protocol": "TCP",
              "sourcePortRange": "*",
              "destinationPortRange": "3389",
              "sourceAddressPrefix": "*",
              "destinationAddressPrefix": "*",
              "access": "Allow",
              "priority": 1000,
              "direction": "Inbound"
            }
          }
        ]
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/ProdataARM/providers/Microsoft.Network/publicIPAddresses/EstebanVM-ip'.",
      "type": "Microsoft.Network/publicIPAddresses",
      "name": "[parameters('publicIPAddresses_EstebanVM_ip_name')]",
      "apiVersion": "2016-03-30",
      "location": "northeurope",
      "properties": {
        "publicIPAllocationMethod": "Static",
        "idleTimeoutInMinutes": 4
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/ProdataARM/providers/Microsoft.Network/virtualNetworks/ProdataARM-vnet'.",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[parameters('virtualNetworks_ProdataARM_vnet_name')]",
      "apiVersion": "2016-03-30",
      "location": "northeurope",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "10.0.0.0/24"
          ]
        },
        "subnets": [
          {
            "name": "default",
            "properties": {
              "addressPrefix": "10.0.0.0/24"
            }
          }
        ]
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/prodataarm/providers/Microsoft.Storage/storageAccounts/prodataarmdiag927'.",
      "type": "Microsoft.Storage/storageAccounts",
      "sku": {
        "name": "Standard_LRS",
        "tier": "Standard"
      },
      "kind": "Storage",
      "name": "[parameters('storageAccounts_prodataarmdiag927_name')]",
      "apiVersion": "2016-01-01",
      "location": "northeurope",
      "tags": {},
      "properties": {},
      "resources": [],
      "dependsOn": []
    },
    {
      "comments": "Generalized from resource: '/subscriptions/5e960efa-b193-495d-9767-e5191a86441c/resourceGroups/prodataarm/providers/Microsoft.Storage/storageAccounts/prodataarmdisks138'.",
      "type": "Microsoft.Storage/storageAccounts",
      "sku": {
        "name": "Standard_LRS",
        "tier": "Standard"
      },
      "kind": "Storage",
      "name": "[parameters('storageAccounts_prodataarmdisks138_name')]",
      "apiVersion": "2016-01-01",
      "location": "northeurope",
      "tags": {},
      "properties": {},
      "resources": [],
